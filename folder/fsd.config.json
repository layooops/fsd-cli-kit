{
  "$schema": "./schema.json",
  "autogenerate": true,
  "globalSettings": {
    "scriptingLanguage": "typescript",
    "styles": {
      "cssFramework": "standard",
      "cssInJsFramework": "styled-components",
      "cssPreprocessor": "css"
    },
    "framework": "react",
    "testing": {
      "enabled": true,
      "testFilePostfix": "test"
    },
    "stateManagement": {
      "type": "none"
    },
    "documentation": {
      "enabled": false,
      "documentTypes": [
        "markdown"
      ]
    }
  },
  "customFiles": [
    {
      "segment": "ui",
      "files": ["./templatesx/ui/react.tsx"]
    },
    {
      "segment": "uixx",
      "files": ["./nosrc/ui/react"]
    }
  ]
}
